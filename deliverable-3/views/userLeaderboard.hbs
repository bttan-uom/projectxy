<div class="head-title">
	<div class="left">
		<ul class="breadcrumb">
			<li>
				<a class="active generic-button" href="#">Leaderboard</a>
			</li>
			<li><i class='bx material-icons'>chevron_right</i></li>
		</ul>
		<br>
		<h1>Leaderboard</h1>
		
	</div>
	
	
</div>

<ul class="box-info">
	<li>
		<i class='bx material-icons'>today</i>
		<span class="text">
			<h3>{{getCurrentDate}}</h3>
			<p>Today's Date</p>
		</span>
	</li>
	<li>
		<i class='bx material-icons'>medication</i>
		<span class="text">
			{{#with clinician}}
				<h3>Dr. {{this.first_name}} {{this.last_name}}</h3>
			{{/with}}
			<p>Your clinician</p>
		</span>
	</li>
</ul>





<div class="table-data">
	<div class="order">
		<div class="head">
			<h3>You are #{{place}} on the leaderboard</h3>
		</div>
		<table class="scrollTable" id="allPatientsTable">
			<thead>
				<tr>
					<th scope="col">User</th>
					<th scope="col">Engagement Rate</th>
				</tr>
			</thead>
			<tbody>
				{{!-- loops over the data set --}}
				{{#each data}}
				<tr>
					<td data-label="Record Type"> 
						<p>{{this.record_type}}</p>
					</td>
					<td data-label="Value"><p>{{this.value}}</p></td>
				</tr>
				{{/each}}
			</tbody>
		</table>
		
		
		
		
		
	</div>
	<div class="todo">
		<div class="head">
			<!--					<i class='bx bx-edit-alt'></i>-->
			<h3>Badges</h3>
		</div>
		<ul class="todo-list">
			<li class="completed">
				<p><img style="width:3rem" src="img/badges/badge1.png"></p>
				<p>ABCDEFG</p>
			</li>
			<li class="completed">
				<p><img style="width:3rem" src="img/badges/badge2.png"></p>
				<p>HIJKLMN</p>
			</li>

		</ul>
		<br>
		<div class="head">
			<h3>This Week</h3>
			
		</div>
		<i class='bx material-icons'>today</i>
	</div>

	
</div>

