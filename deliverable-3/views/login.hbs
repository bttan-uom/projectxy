<div class="head-title">
	<div class="left">
		<ul class="breadcrumb">
			<li>
				<a class="active generic-button" href="#">Login</a>
			</li>
			<li><i class='bx material-icons'>chevron_right</i></li>
		</ul>
		<br>
		<h1>Login to MyGlucose</h1>
		
	</div>
	

</div>

<div class="table-data">
	<div class="order">
		<div class="head">
<!--					<i class='bx bx-check-square' ></i>-->
			<h3>Login</h3>
		</div>
		{{#each flash}}
			<p class="alert">{{this}}</p>
		{{/each}}
		<form class="form-group" action="auth" method="POST">
			<label>Username</label>
			<input class="input-button-select" type="text" placeholder="Username" name="username" />
			<br><br>
			<label>Password</label>
			<input class="input-button-select" type="password" placeholder="Password" name="password" />
			<br><br>
			<input style="font-size: 110%;" class="generic-button" type="submit" value="submit" />
		</form>

		<br>
	</div>
	<div class="todo">
		<div class="head">
			<!--					<i class='bx bx-edit-alt'></i>-->
			<h3>Title</h3>
		</div>
			<img style="width:20rem" src="../img/aboutusbox1.png">
				
	</div>
</div>