<body style="background-color:#FFE0D3;">
	<div class="head-title" >
		<div class="left">
			<ul class="breadcrumb">
				<li>
					<a class="active generic-button" href="/clinician/notes">+Notes</a>
				</li>

				<li>
					<i class='bx material-icons'>chevron_right</i>
				</li>

				<li>
					<a class="active generic-button" href="#">Note not saved</a>
				</li>
			</ul>
			<br>
			<h1>Note could not be saved <i style="font-size: 100%;" class='bx material-icons'>block</i></h1>
		</div>
	</div>

	<ul class="box-info">
		<li>
			<i class='bx material-icons'>today</i>
			<span class="text">
				<h3 id="CurrentDate"></h3>
				<p>Today's Date</p>
			</span>
		</li>
	</ul>
	
	<div class="table-data">
		<div class="order">
			<div class="head">
				<i style="font-size: 500%;" class='bx material-icons'>block</i>
			</div>

			{{#with error}}
			<p>Error: {{this}}</p>
			{{/with}}
		</div>
	</div>

	<br><br>
	<a href="/clinician/notes">
		<button style="padding: 0.8rem 22%;" class="generic-button" ><i style="font-size: 250%;" class='bx material-icons'>check</i><p style="font-size:180%">Done</p></button>
	</a> 
	
</body>