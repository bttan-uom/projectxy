
<div class="head-title">
    <div class="left">
        <ul class="breadcrumb">
            <li>
                <pre><a class="active generic-button" href="/clinicianNotes">Patients</a></pre>
            </li>
            <li><i class='bx material-icons'>chevron_right</i></li>
            <li>
                <pre><a class="active generic-button" href="#">View Patient {{name}}</a></pre>
            </li>
        </ul>
        <br>
        <h1>View Patient {{name}}</h1>
    </div>
</div>


<ul class="box-info">
    <li>
        <i class='bx material-icons'>today</i>
        <span class="text">
            <h3>{{getCurrentDate}}</h3>
            <p>Current Date</p>
        </span>
    </li>
    <li>
        <i class='bx material-icons'>personal_injury</i>
        <span class="text">
            <h3>{{time}}</h3>
            <p>Last record timestamp</p>
        </span>
    </li>
    <li>
        <i style="background: var(--light-orange); color: var(--orange);" class='bx material-icons'>assignment_late</i>
        <span class="text">
            <h3>{{Num}}</h3>
            <p>Incomplete entries today</p>
        </span>
    </li>
    <li>
        <i style="background: var(--light-orange); color: var(--orange);" class='bx material-icons'>data_thresholding</i>
        <span class="text">
            <h3>{{Num}}</h3>
            <p>Threshold warnings today</p>
        </span>
    </li>
</ul>




<div class="table-data">
    <div class="order">
        <div class="head">
            <!--					<i class='bx bx-check-square' ></i>-->
            <h3>Patient Details</h3>
            <i class='bx material-icons'>edit</i>
        </div>
        
        
        <ul class="box-info">
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Full Name</h3>
                    <h3>{{this.something}}</h3>
                </span>
                
            </li>
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Date of sign-up</h3>
                    <h2>{{formatLongDate this.created_at}}</h2>
                </span>
            </li>
        </ul>
        <br>
        <ul class="box-info">
            
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Phone number</h3>
                    <h3>{{this.something}}</h3>
                </span>
            </li>
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Email/username</h3>
                    <h3>{{this.something}}</h3>
                </span>
            </li>
        </ul>
        <ul class="box-info">
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Address</h3>
                    <h3>{{this.something}}</h3>
                </span>
                
            </li>
        </ul>
        <br>
        <ul class="box-info">
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Height</h3>
                    <h3>{{this.something}}</h3>
                </span>
            </li>
            
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Date of Birth</h3>
                    <h3>{{this.something}}</h3>
                </span>
            </li>
            <li style="background-color:var(--grey);">
                <span class="text">
                    <h3>Gender</h3>
                    <h3>{{this.something}}</h3>
                </span>
            </li>
        </ul>
        

        
    </div>
    <div class="todo">
        <div class="head">
            <!--					<i class='bx bx-edit-alt'></i>-->
            <h3>Patient details</h3>
        </div>
        <ul class="todo-list">
            <li class="blueone">
                <p>Records being recorded:</p>
                <p>One, two, three, four</p>
            </li>
        </ul>
        <br>
        <div class="head">
            <!--					<i class='bx bx-edit-alt'></i>-->
            <h3>Patient thresholds</h3>
        </div>
        <ul class="todo-list">
            <li class="blueone">
                <p>Blood Glucose: 3.9 - 5.6 nMol/L</p>
            </li>
            <li class="blueone">
                <p>Insulin Doses: 2 Doses</p>
            </li>
            <li class="blueone">
                <p>Weight: 65 - 75 kg</p>
            </li>
            <li class="blueone">
                <p>Exercise: At least 5000 steps</p>
            </li>
        </ul>

    </div>
</div>
